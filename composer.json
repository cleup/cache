{
    "name": "cleup/cache",
    "version": "1.0.2",
    "description": "Flexible caching library for PHP with multiple drivers support",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/cleup/guard",
    "authors": [
        {
            "name": "Eduard Y",
            "email": "dev@priveted.com"
        }
    ],
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "Cleup\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cleup\\Cache\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^7.3"
    },
    "suggest": {
        "ext-igbinary": "For better serialization performance",
        "ext-redis": "For Redis to work correctly",
        "ext-memcached": "For Memcached to work correctly"
    }
}
